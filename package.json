{
  "name": "voip-backend",
  "version": "1.0.0",
  "author": "@flaviozno and @jaonolo",
  "description": "O backend respons√°vel por fazer o Queemo funcionar!",
  "main": "dist/server.js",
  "scripts": {
    "socket": "ts-node-dev --respawn --transpile-only src/server.ts",
    "peerjs": "ts-node-dev --respawn --transpile-only src/peer.ts",
    "dev": "concurrently \"npm run peerjs\" \"npm run socket\"",
    "build": "tsc",
    "start:peerjs": "node dist/peer.js",
    "start:socket": "node dist/server.js",
    "start": "concurrently \"npm run start:peerjs\" \"npm run start:socket\"",
    "test": "jest --detectOpenHandles"
  },
  "keywords": [
    "voip",
    "league-of-legends",
    "webrtc",
    "peerjs",
    "socket.io",
    "nodejs",
    "typescript"
  ],
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^29.5.14",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "jest": "^29.7.0",
    "logger": "github:jerobas/logger",
    "peer": "^1.0.2",
    "socket.io": "^4.8.1",
    "ts-jest": "^29.3.4",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.4",
    "@types/socket.io": "^3.0.2",
    "concurrently": "^9.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
