{
  "name": "voip-backend",
  "version": "1.0.0",
  "description": "Backend for VOIP app",
  "main": "dist/server.js",
  "scripts": {
    "ssh": "ssh -i ../ssh.pem -R 0.0.0.0:3001:localhost:3001 -R 0.0.0.0:3002:localhost:3002 ubuntu@ec2-15-228-45-137.sa-east-1.compute.amazonaws.com",
    "socket": "ts-node-dev --respawn --transpile-only src/server.ts",
    "peerjs": "ts-node-dev --respawn --transpile-only src/peer.ts",
    "dev": "concurrently \"npm run peerjs\" \"npm run socket\" \"npm run ssh\"",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "peer": "^1.0.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.4",
    "@types/socket.io": "^3.0.2",
    "concurrently": "^9.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}